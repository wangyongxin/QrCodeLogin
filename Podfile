# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'
source 'https://github.com/CocoaPods/Specs.git'

target 'QrCodeLogin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for QrCodeLogin
  
  # https://cocoapods.org/pods/QRCodeReader.swift
  pod 'QRCodeReader.swift', '~> 10.1.0'
  # - Homepage: https://github.com/marcuswestin/WebViewJavascriptBridge
  # - Source:   https://github.com/marcuswestin/WebViewJavascriptBridge.git
  pod 'WebViewJavascriptBridge', '~> 6.0.3'
  pod 'Alamofire', '~> 4.8.2'

  target 'QrCodeLoginTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'QrCodeLoginUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
